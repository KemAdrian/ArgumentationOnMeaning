(define (sponge)
  (spiculate-skeleton (define (spiculate-skeleton)
                        (uniform-length no)
                        (chemical silica)
                        (megascleres (define (megascleres)
                                       (acanthose yes)
                                       (ornamentation (define (set)
                                                        smooth-ornamentation
                                                        spines))
                                       (smooth-form (define (set)
                                                      style
                                                      subtylostyle
                                                      tylostyle))
                                       (max-length 1200)
                                       (two-or-more-length no)))
                        (architecture plumose)
                        (sclerospongids no)))
  (ecological-features (define (ecological-features)
                         (substrate rock)
                         (fixation totally)
                         (location mediterranean)))
  (external-features (define (external-features)
                       (hollow no)
                       (boring no)
                       (colour (define (colour)
                                 (external red)))
                       (body-size small)
                       (consistency soft)
                       (growing (define (growing)
                                  (grow (define (set)
                                          erect
                                          encrusting))
                                  (line-form (define (line-form)
                                               (peduncle no)
                                               (form branching)))))
                       (osc (define (absent-osc)))
                       (hand flexible)
                       (touch (define (set)
                                hispid
                                smooth-touch))
                       (briozoa no)
                       (surface even)
                       (crible no)
                       (brush no)
                       (macro-debris some)
                       (gemmules no)))
  (anatomy (define (anatomic-features)
             (ornamentation (define (set)
                              smooth-ornamentation
                              spines))
             (ectosome no-visible)
             (cortex (define (cortex-characteristics))))))
