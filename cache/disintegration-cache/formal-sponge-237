(define (sponge)
  (spiculate-skeleton (define (spiculate-skeleton)
                        (uniform-length no)
                        (chemical silica)
                        (megascleres (define (megascleres)
                                       (acanthose no-acanthose)
                                       (ornamentation smooth-ornamentation)
                                       (smooth-form (define (set)
                                                      style
                                                      strongyle))
                                       (max-length 1300)
                                       (two-or-more-length no)
                                       (predominant-kind styles)
                                       (styloids no)))
                        (sclerospongids no)))
  (ecological-features (define (ecological-features)
                         (association no)
                         (location mediterranean)))
  (external-features (define (external-features)
                       (hollow no)
                       (boring no)
                       (colour (define (colour)
                                 (external orange-to-red)))
                       (body-size medium)
                       (consistency soft)
                       (growing (define (growing)
                                  (grow erect)
                                  (line-form (define (line-form)
                                               (peduncle no)
                                               (form (define (set)
                                                       foliaceus
                                                       branching))))))
                       (osc (define (absent-osc)))
                       (hand rubbery)
                       (briozoa no)
                       (surface even)
                       (crible no)
                       (brush no)
                       (macro-debris none)
                       (gemmules no)))
  (anatomy (define (anatomic-features)
             (ornamentation smooth-ornamentation)
             (ectosome (define (visible-ectosome)
                         (separable no)
                         (spicules no)))
             (cortex (define (absent-cortex))))))
