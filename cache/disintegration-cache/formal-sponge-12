(define (sponge)
  (spiculate-skeleton (define (spiculate-skeleton)
                        (uniform-length no)
                        (microscleres absent-microscleres)
                        (chemical silica)
                        (megascleres (define (megascleres)
                                       (acanthose no-acanthose)
                                       (ornamentation smooth-ornamentation)
                                       (smooth-form (define (set)
                                                      oxea
                                                      (define (orthotriaena)
                                                       (max-diameter 40))
                                                      protriaena))
                                       (max-length 2500)
                                       (two-or-more-length yes)
                                       (characteristics (define (characters)
                                                          (shaft (define (set)
                                                                   normal
                                                                   fusiform))))))
                        (architecture radiate)
                        (sclerospongids no)))
  (ecological-features (define (ecological-features)
                         (location mediterranean)))
  (external-features (define (external-features)
                       (osc (define (osc-characteristics)
                              (dot yes)
                              (pososc depressed)
                              (numosc several)))
                       (hand (define (set)
                               tough
                               breakable))
                       (pigmentation no)
                       (touch (define (set)
                                hispid
                                rough))
                       (hollow no)
                       (surface (define (set)
                                  incrusted-pebbles-or-calcareous-debris
                                  hispid
                                  even))
                       (colour (define (colour)
                                 (internal yellowish)
                                 (external (define (set)
                                             grey
                                             whitish))))
                       (body-size medium)
                       (brush no)
                       (macro-debris some)
                       (consistency stony)
                       (growing (define (growing)
                                  (line-form (define (line-form)
                                               (peduncle no)
                                               (line circular)
                                               (form hemispherical)))))))
  (anatomy (define (anatomic-features)
             (ornamentation smooth-ornamentation)
             (ectosome (define (visible-ectosome)
                         (separable yes)
                         (spicules yes)))
             (cortex (define (cortex-characteristics)
                       (cortical-spicules yes)
                       (thick 2000))))))
