  (spiculate-skeleton (define (spiculate-skeleton)
                        (megascleres (define (megascleres)
                                       (acanthose no-acanthose)
                                       (ornamentation smooth-ornamentation)
                                       (smooth-form (define (set)
                                                      oxea
                                                      (define (orthotriaena)
                                                       (max-diameter 40))
                                                      protriaena))
                                       (max-length 2500)
                                       (two-or-more-length yes)
                                       (characteristics (define (characters)
                                                          (shaft normal)))))))
  (external-features (define (external-features)
                       (osc (define (osc-characteristics)
                              (dot yes)
                              (pososc depressed)
                              (numosc several)))
                       (colour (define (colour)
                                 (internal yellowish)
                                 (external grey)))
                       (body-size (define (body-size)))
                       (macro-debris some)
                       (consistency stony)
                       (growing (define (growing)
                                  (line-form (define (line-form)
                                               (peduncle no)
                                               (line circular)
                                               (form hemispherical)))))))
  (anatomy (define (anatomic-features)
             (ornamentation smooth-ornamentation)
             (ectosome (define (visible-ectosome)
                         (separable yes)
                         (spicules yes)))
             (cortex (define (cortex-characteristics)
                       (cortical-spicules yes)
                       (thick 2000))))))
